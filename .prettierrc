{
    "singleQuote": true,
    "trailingComma": "all",
    "printWidth": 100,
    "tabWidth": 4,
    "semi": true,
    "bracketSpacing": true,
    "arrowParens": "avoid",
"overrides": [
			{
				"files": "*.html",
				"options": {
					"parser": "html"
				}
			},
			{
				"files": "*.component.html",
				"options": {
					"parser":"angular"
				}
			}
		]
}